<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/joinclube_00_artboard_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/artboard_main_ver0"
    tools:ignore="MissingDefaultResource">


    <ImageButton
        android:id="@+id/joinclube_00_imagebutton_back"

        android:layout_width="0dp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.051803128"
        app:layout_constraintHorizontal_bias="0.113298068"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.051296875"
        app:layout_constraintVertical_bias="0.051875
"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/ic_icon_ionic_ios_arrow_back_ver0"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/joinclube_00_artboard_sub"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="160dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="1"
        app:layout_constraintHorizontal_bias="0.5"


        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="1"
        app:srcCompat="@drawable/artboard_sub_su_ver0"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/joinclube_00_imageview_logo"
        android:layout_width="0dp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.426715731"
        app:layout_constraintHorizontal_bias="0.500386385"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.066666667"
        app:layout_constraintVertical_bias="0.051875"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/wegloo_logo_ver0"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/joinclube_00_textview_pickclub"
        android:layout_width="0dp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.580404784"
        app:layout_constraintHorizontal_bias="0.5"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.0546875"
        app:layout_constraintVertical_bias="0.287"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="모임을 선택 해주세요!"
        android:gravity="center"
        android:autoSizeMinTextSize="2sp"
        android:autoSizeMaxTextSize="20dp"
        android:autoSizeTextType="uniform"
        />


    <com.example.target_club_in_donga.club_foundation_join.CustomAutoCompleteTextView
        android:dropDownAnchor="@id/joinclube_autoCompleteSearch"
        android:dropDownHeight="200dp"

        android:id="@+id/joinclube_autoCompleteSearch"
        android:hint = "모임 검색 (2자 이상)"
        android:completionThreshold="1"
        android:textColorHint="@color/color_Orange"
        android:theme="@style/EditTheme"
        android:layout_width="0dp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.661113155"
        app:layout_constraintHorizontal_bias="0.5"
        android:layout_height="wrap_content"
        app:layout_constraintVertical_bias="0"

        android:layout_marginTop="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/joinclube_00_textview_pickclub"

        />

    <EditText
        android:id="@+id/joinclube_edittext_content"
        android:hint="자기 소개글 (70자 이내)\n모임 임원이 확인하는 정보입니다."
        android:gravity="start"
        android:textSize="14dp"
        android:inputType="textMultiLine"
        android:textColorHint="@color/color_Orange"
        android:theme="@style/EditTheme"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:maxLength="70"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintVertical_bias="0.7"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/joinclube_autoCompleteSearch"
        app:layout_constraintStart_toStartOf="@+id/joinclube_autoCompleteSearch"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/joinclube_button_next"

        android:layout_width="0dp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.776448942"
        app:layout_constraintHorizontal_bias="0.5"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.0640625"
        app:layout_constraintVertical_bias="0.86953125"

        android:background="@drawable/logoin_button_ver0"
        android:elevation="5dp"
        android:text="모임 정보 확인하기"
        android:textColor="@color/color_White"
        android:autoSizeMinTextSize="1sp"
        android:autoSizeMaxTextSize="20sp"
        android:autoSizeTextType="uniform"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>